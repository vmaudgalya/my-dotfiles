don't care how the object is named */\n\t\t\t\tif(activity.extensions) {\n\t\t\t\t\t/* Get the extension key. \"payment_properties\", \"invoice_properties\" etc.\n\t\t\t\t\t * There will always be only one.\n\t\t\t\t\t */\n\t\t\t\t\textensionKey = (extensionKeys = _.keys(activity.extensions)) && extensionKeys && extensionKeys.length? extensionKeys[0] : null;\n\t\t\t\t\tactivityExtension = extensionKey? activity.extensions[extensionKey] : null;\n\t\t\t\t}\n\t\t\t\tif(auxInfoEntry && auxInfoEntry.extensions) {\n\t\t\t\t\t/* Get the extension key. \"payment_properties\", \"invoice_properties\" etc.\n\t\t\t\t\t * There will always be only one.\n\t\t\t\t\t */\n\t\t\t\t\textensionKey = (extensionKeys = _.keys(auxInfoEntry.extensions)) && extensionKeys && extensionKeys.length? extensionKeys[0] : null;\n\t\t\t\t\tauxInfoEntryExtension = extensionKey? auxInfoEntry.extensions[extensionKey] : null;\n\t\t\t\t}\n\n\t\t\t\ttempTransactionStatus = that.getStatus(activity, activityExtension);\n\n\t\t\t\t/* Start filling up the model */\n\t\t\t\t_.extend(activityModel,\n\n\t\t\t\t\t/* Basic info */\n\t\t\t\t\t{\n\t\t\t\t\t\tinternalTransactionId: activity.internal_id,\n\t\t\t\t\t\ttransactionId: activity.id,\n\t\t\t\t\t\ttransactionTime: that.getTransactionTime(activity),\n\t\t\t\t\t\ttransactionTimeUnformatted: activity.time_created,\n\t\t\t\t\t\ttransactionType: activity.activity_type,\n\t\t\t\t\t\ttransactionStatus: tempTransactionStatus,\n\t\t\t\t\t\ttransactionParent:activity.parent_id? activity.parent_id : \"None\",\n\t\t\t\t\t},\n\n\n\t\t\t\t\t/* Balance and fee related info */\n\t\t\t\t\tthat.getAmountsForActivity(activity, activityExtension, populateBalance),\n\n\t\t\t\t\t/* Note */\n\t\t\t\t\tthat.getNote(activityExtension, auxInfoEntryExtension),\n\n\t\t\t\t\t/* Shipping Info */\n\t\t\t\t\tthat.getShippingInfo(activityExtension, auxInfoEntryExtension)\n\t\t\t\t);\n\n\t\t\t\t/* Build the action list.\n\t\t\t\t * If auxInfo exists from the batch call, use that as the source,\n\t\t\t\t * otherwise use the activity object.\n\t\t\t\t */\n\n\t\t\t\t/* Secondary transactions */\n\t\t\t\tthat.getSecondaryTransactionFromHash(activityModel,populateBalance,result,secondaryAuxInfo,function(activityModel,secondaryTxns){\n\t\t\t\t\tactivityModel.secondaryTransactions=secondaryTxns;\n\t\t\t\t});\n\n\t\t\t\tactivityModel.actionList = that.buildActionList.apply(that,\n\t\t\t\t\tauxInfoEntry ? [auxInfoEntry, auxInfoEntryExtension, isPrimaryTxn] : [activity, activityExtension, isPrimaryTxn]);\n\n\t\t\t\t/* Sort the actions */\n\t\t\t\tactionListSortFunction = that.getActionListSortFunction(activity.subtype, activity.activity_type);\n\t\t\t\tif (actionListSortFunction) {\n\t\t\t\t\tactivityModel.actionList.sort(actionListSortFunction);\n\t\t\t\t}\n\n\t\t\t\t/* Figure out PUI */\n\t\t\t\tif (activityExtension && activityExtension.delayed_payment_types) {\n\t\t\t\t\tif (activityExtension.delayed_payment_types && activityExtension.delayed_payment_types.indexOf(\"PAY_UPON_INVOICE\") > -1 &&\n\t\t\t\t\t\tactivityExtension.debit_credit_code === 'CREDIT') // No Payment received activity type in new api\n\t\t\t\t\t{\n\t\t\t\t\t\tactivityModel.isPayUponInvoice = true;\n\t\t\t\t\t}\n\t\t\t\t}\n\n\t\t\t\t/* Get the transaction description */\n\t\t\t\tthat.getTransactionDescription(activity, function(info) {\n\t\t\t\t\tactivityModel.transactionDescription = info;\n\n\t\t\t\t\t/* Update the model */\n\t\t\t\t\ttempModel.push(activityModel);\n\t\t\t\t\tdone();\n\t\t\t\t});\n\t\t\t}\n\t\t}, function() {\n\t\t\tdonePopulating(tempModel);\n\t\t});\n\t},\n\n\t/* To populate secondary transactions, which is reutrned as an array to the primary transaction that called */\n\tgetSecondaryTransactionFromHash:function(activityModel, populateBalance, result, secondaryAuxInfo, callback){\n\t\tif(secondaryAuxInfo.secondaryHash){\n\t\t\tif(secondaryAuxInfo.secondaryHash[activityModel.transactionId]){\n\t\t\t\tsecondaryAuxInfo.isPrimaryTxn=false;\n\t\t\t\tthis.populateActivities(populateBalance,secondaryAuxInfo.secondaryHash[activityModel.transactionId], secondaryAuxInfo, function(tempModel){\n\t\t\t\t\tcallback(activityModel,tempModel);\n\t\t\t\t});\n\t\t\t}else{\n\t\t\t\tsecondaryAuxInfo.isPrimaryTxn=true;\n\t\t\t\tcallback(activityModel,[]);\n\t\t\t}\n\t\t}\n\t\telse{\n\t\t\tcallback(activityModel,[]);\n\t\t}\n\t},\n\n\t/* Figures out the status */\n\tgetStatus: function(activity, activityExtension) {\n\t\t/* We should be showing \"under review\" only when transactions have a TRASACTION_LEVEL_HOLD */\n\t\tvar is_under_review_hold = (activityExtension && Array.isArray(activityExtension.holds_info))? _.some(activityExtension.holds_info, function(hold_info) {\n\t\t\treturn hold_info.hold_reason === \"TRANSACTION_LEVEL_HOLD\";\n\t\t}) : false;\n\n\t\tif (activity.detailed_status === \"ECHECK_PENDING\" && activityExtension && activityExtension.hasOwnProperty('settlement_time')) {\n\t\t\treturn this.contentBundle.activity.ACTIVITYSEARCHSERV_STATUS[activity.detailed_status] + \" \" +\n\t\t\t\tthis.locality.griffin.formatDate(Moment(activity.settlement_time).tz(this.locality.timezone).toDate(), this.locality.griffin.DATE_FORMAT_MEDIUM);\n\t\t} else if(activity.detailed_status === \"UNDER_REVIEW\" && !is_under_review_hold) {\n\t\t\treturn this.contentBundle.activity.ACTIVITYSEARCHSERV_STATUS.COMPLETED;\n\t\t} else {\n\t\t\treturn this.contentBundle.activity.ACTIVITYSEARCHSERV_STATUS[activity.detailed_status];\n\t\t}\n\t},\n\n\t/**\n\t * @name: getTransactionTime\n\t * @brief: populates the activities action model the transaction creation time/date\n\t * @param: {Object} activity\n\t **/\n\tgetTransactionTime: function(activity) {\n\n\t\t/* Get dates in user locale's timezones */\n\t\tvar currentDate = Moment().tz(this.locality.timezone),\n\t\t\ttransactionDate = Moment.tz(activity.time_created, this.locality.timezone),\n\t\t\tdisplayFormat = this.locality.griffin.DATE_FORMAT_MEDIUM;\n\n\t\t/* Compare the dates. If the transaction occurs in the current day, show the time */\n\t\tif ((currentDate.date() === transactionDate.date()) &&\n\t\t\t(currentDate.month() === transactionDate.month()) &&\n\t\t\t(currentDate.year() === transactionDate.year())) {\n\n\t\t\tdisplayFormat = this.locality.griffin.TIME_FORMAT_SHORT;\n\n\t\t}\n\n\t\treturn this.locality.griffin.formatDate({\n\t\t\tdate: transactionDate.toISOString(),\n\t\t\ttimeZone: this.locality.timezone\n\t\t}, displayFormat);\n\t},\n\n\t/**\n\t * @name: addArchiveActions\n\t * @brief: populates the activities action model with archive actions\n\t * @param: {Object} activity\n\t * @param: {Object} actions to add\n\t **/\n\taddArchiveActions: function(activity) {\n\t\tvar archive = null;\n\t\tif (activity.tags && activity.tags.indexOf(TxnConstants.ARCHIVE_TAG) !== -1) {\n\t\t\t/* If this transaction is hidden, give the ability to unarchive(show) */\n\t\t\tarchive = TxnConstants.UNARCHIVE_TAG;\n\t\t} else {\n\t\t\t/* If no tags exist or if HIDE doesn't exist, give the ablity to archive (hide) */\n\t\t\tarchive = TxnConstants.ARCHIVE_TAG;\n\t\t}\n\n\t\tif (archive) {\n\t\t\treturn {\n\t\t\t\tname: this.contentBundle.tags[archive],\n\t\t\t\ttype: archive\n\t\t\t};\n\t\t}\n\t\treturn null;\n\t},\n\n\t/**\n\t * @name: getAmountsForActivity\n\t * @brief: populates the activities model with amounts and fees\n\t * @param: {Object} activity {Boolean} populateBalance\n\t * @param: {Object} result\n\t **/\n\tgetAmountsForActivity: function(activity, activityExtension, populateBalance) {\n\t\tvar isDebitTransaction = false,\n\t\t\tamountInfo = {};\n\n\t\tif (activity && activity.gross) {\n\t\t\t//Check if amount is negative\n\t\t\tisDebitTransaction = activityExtension ? activityExtension.debit_credit_code === \"DEBIT\" : false;\n\t\t\t_.extend(amountInfo, {\n\t\t\t\tgrossAmount: (new AmountModel(Utils.negateDebitCreditAmount(activity.gross, isDebitTransaction),\n\t\t\t\t\tthis.locality.griffin)).getFormattedAmountInMajorCurrency()\n\t\t\t}, populateBalance ? {\n\t\t\t\t//We show these amount categories only in Bookkeeping tab.\n\t\t\t\t//Transaction Fees\n\t\t\t\tfeeAmount: (activity.fee) ?\n\t\t\t\t\t(new AmountModel(Utils.negateDebitCreditAmount(activity.fee, true /*isNegativeAmount*/ ),\n\t\t\t\t\t\tthis.locality.griffin)).getFormattedAmountInMajorCurrency() : '',\n\n\t\t\t\t//Transaction net amount\n\t\t\t\tnetAmount: (activity.net) ?\n\t\t\t\t\t(new AmountModel(Utils.negateDebitCreditAmount(activity.net, isDebitTransaction),\n\t\t\t\t\t\tthis.locality.griffin)).getFormattedAmountInMajorCurrency() : '',\n\n\t\t\t\t//Running balance after the transaction\n\t\t\t\tbalanceAmount: (activityExtension && activityExtension.running_balance) ?\n\t\t\t\t\t(new AmountModel(activityExtension.running_balance, this.locality.griffin)).getFormattedAmountInMajorCurrency() : ''\n\n\t\t\t} : {});\n\t\t}\n\n\t\treturn amountInfo;\n\t},\n\n\t/**\n\t * @name: getNote\n\t * @brief: populates the activities model with note and description\n\t * @param: {Object} activity\n\t * @param: {Object} result\n\t **/\n\n\tgetNote: function(activityExtension, auxInfoEntryExtension) {\n\t\tvar noteInfo = {},\n\t\t\textensionSource = null;\n\n\t\t/* If the activity object contains the buyer_notes, use that as the source. Otherwise,\n\t\t * look into the auxInfo object from the batch call\n\t\t */\n\t\tif (activityExtension && activityExtension.buyer_notes) {\n\t\t\textensionSource = activityExtension;\n\t\t} else {\n\t\t\textensionSource = auxInfoEntryExtension;\n\t\t}\n\n\t\tif (extensionSource) {\n\t\t\tnoteInfo.transactionNote = (extensionSource.buyer_notes) ? extensionSource.buyer_notes : null;\n\t\t}\n\n\t\treturn noteInfo;\n\t},\n\n\t/**\n\t * @name: getShippingInfo\n\t * @brief: populates the activities model with shipping info\n\t * @param: {Object} activity {Object} auxInfo from batch call post-search\n\t * @param: {Object} result\n\t **/\n\n\tgetShippingInfo: function(activityExtension, auxInfoEntryExtension) {\n\n\t\tvar extensionForShipping = null,\n\t\t\tshippingInfo = {};\n\n\t\t/* If the activity object contains the shipping status, use that as the source. Otherwise,\n\t\t * look into the auxInfo object from the batch call\n\t\t */\n\t\tif (activityExtension && activityExtension.shipping_status) {\n\t\t\textensionForShipping = activityExtension;\n\t\t} else {\n\t\t\textensionForShipping = auxInfoEntryExtension ? auxInfoEntryExtension : null;\n\t\t}\n\n\t\tif (extensionForShipping) {\n\t\t\tshippingInfo.shippingStatus = {\n\t\t\t\tstatusId: extensionForShipping.shipping_status,\n\t\t\t\tstatusLabel: this.contentBundle.activity.SHIPPING[extensionForShipping.shipping_status]\n\t\t\t};\n\t\t}\n\n\t\treturn shippingInfo;\n\t},\n\n\t/**\n\t * @name: ActivitySearchServCallback\n\t * @brief: Callback function for waletActivityServ call. Builds the transactions activity model.\n\t * @param: {Object} err\n\t * @param: {Object} result\n\t * @param: {function} callback\n\t **/\n\n\tactivitySearchServCallback: function(populateBalance, err, result, secondaryAuxInfo, callback) {\n\t\tvar serviceErrorHandler,\n\t\t\tthat = this;\n\t\tif (!err && result && result.statusCode === 200) {\n\t\t\tif (Array.isArray(result.body.items)) {\n\t\t\t\t//Check if there is no user activity\n\t\t\t\tif (result.body.items.length < 1) {\n\t\t\t\t\tcal.logInfo('ACTIVITYSEARCHSERV_CALL_SUCCESS', 'No Results');\n\t\t\t\t\tthis.transactionModel.message = this.contentBundle.activity.listErrorNoResultsForSearch;\n\t\t\t\t\tthis.transactionModel.transactions = null;\n\t\t\t\t\tcallback(null, this.transactionModel);\n\t\t\t\t} else {\n\t\t\t\t\tcal.logInfo('ACTIVITYSEARCHSERV_CALL_SUCCESS', {\n\t\t\t\t\t\t'no of records returned': result.body.items.length\n\t\t\t\t\t});\n\n\t\t\t\t\t/* Fetch next page info */\n\t\t\t\t\t_.extend(this.transactionModel, this.getNextPageLink(result));\n\t\t\t\t\tthis.populateActivities(populateBalance, result.body.items, secondaryAuxInfo, function(tempModel) {\n\t\t\t\t\t\tthat.transactionModel.transactions=tempModel;\n\t\t\t\t\t\tcallback(null, that.transactionModel);\n\t\t\t\t\t});\n\t\t\t\t}\n\t\t\t}\n\t\t} else { // Activitysearchserv call failed.\n\t\t\tif (result && result.body && result.body.details) { // Activitysearchserv is populating error info in result.body.details for any validation errors.\n\t\t\t\tcal.logAsSystemError(TxnConstants.CalErrorMessage + \"Transactions Page: activityMiddleware error\");\n\t\t\t\tcal.logAsSystemError('ACTIVITYSEARCHSERV_CALL_FAILED', result.body.details);\n\t\t\t}\n\t\t\tserviceErrorHandler = new ServiceErrorHandler();\n\t\t\tserviceErrorHandler.handle(err, result, 'transactionsMiddleware.activitysearchserv::getActivities callback');\n\t\t\tcallback(serviceErrorHandler, null);\n\t\t}\n\t},\n\n\tgetNextPageLink: function(result) {\n\t\tvar resultLinks = null;\n\t\tvar nextLinkInfo = {\n\t\t\thasMore: false,\n\t\t\tnextpageurl: null\n\t\t};\n\t\tvar rex,regx,checkToken;\n\t\t/* Find the next page token. activitysearchserv returns\n\t\t * a URL (which we unfortunately can't consume as it is). The activities endpoint returns\n\t\t * 'next_page_token', and elastic search returns 'page'. This is the ugly logic to parse\n\t\t * that stuff out.\n\t\t */\n\t\tif (Array.isArray(result.body.links) && result.body.links.length > 0) {\n\t\t\tresultLinks = result.body.links.shift();\n\t\t\tnextLinkInfo.hasMore = (resultLinks.href) ? true : false;\n\t\t\t// HATEOS link to the next page, if there is a next page , does this really exist\n\n\t\t\t/* Instead of passing the entire url, pass just the next page token through the model */\n\t\t\trex=TxnConstants.ASS_NEXT_PAGE_URL_PARAM+\"=(.*?)(&|$)\";\n\t\t\tregx=new RegExp(rex,'g');\n\t\t\tif(resultLinks.href) {\n\t\t\t\t    checkToken = regx.exec(resultLinks.href);\n\t\t\t\t    if(Array.isArray(checkToken) && checkToken[1]) {\n\t\t\t\t        nextLinkInfo.nextpageurl = checkToken[1];\n\t\t\t\t   \t}else{\n\t\t\t\t   \t\tnextLinkInfo.nextpageurl = null;\n\t\t\t\t   \t}\n\t\t\t\t}\n\t\t\t\telse {\n\t\t\t\t\tnextLinkInfo.nextpageurl = null;\n\t\t\t\t}\n\n\t\t\t/* The activities and search endpoints return the next page tokens with\n\t\t\t * a different name. Activities returns 'next_page_token' and search returns 'page'.\n\t\t\t * Activitysearchserv plans to make this more streamlined. Ideally we would be using\n\t\t\t * the URL they return as a whole (But servicecore etc etc).\n\t\t\t */\n\t\t\t if(!nextLinkInfo.nextpageurl){\n\t\t\t \trex=TxnConstants.EASS_NEXT_PAGE_URL_PARAM+\"=(.*?)(&|$)\";\n\t\t\t\tregx=new RegExp(rex,'g');\n\t\t\t\tif(resultLinks.href) {\n\t\t\t\t    checkToken = regx.exec(resultLinks.href);\n\t\t\t\t    if(Array.isArray(checkToken) && checkToken[1]) {\n\t\t\t\t        nextLinkInfo.nextpageurl = checkToken[1];\n\t\t\t\t   \t}else{\n\t\t\t\t   \t\tnextLinkInfo.nextpageurl = null;\n\t\t\t\t   \t}\n\t\t\t\t}\n\t\t\t\telse {\n\t\t\t\t\tnextLinkInfo.nextpageurl = null;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\n\t\treturn nextLinkInfo;\n\t},\n\n\tgetTransactionDescription: function(activity, callback) {\n\t\tvar name = '',\n\t\t\tthat = this,\n\t\t\tdescription = '';\n\n\t\tactivityReader.read(activity, function(activityInfo) {\n\n\t\t\t/* Get the description */\n\t\t\tif (activityInfo.description && activityInfo.description.key && activityInfo.description.key !== '') {\n\t\t\t\tdescription = that.contentBundle.activity[activityInfo.description.key];\n\t\t\t}\n\n\t\t\t/* Get the counterparty */\n\t\t\tif (activityInfo.counterparty && activityInfo.counterparty.business !== '') {\n\t\t\t\tname = activityInfo.counterparty.business;\n\t\t\t}\n\n\t\t\t/* Figure out if any special handling needs to be done */\n\n\t\t\t/* FI content */\n\t\t\tif (activityInfo.description.flags.NEEDS_CP_RESOLUTION) {\n\t\t\t\tif(that.contentBundle.activity['FI_' + activityInfo.counterparty.fi]) {\n\t\t\t\t\tname = that.contentBundle.activity['FI_' + activityInfo.counterparty.fi];\n\t\t\t\t}\n\t\t\t}\n\n\t\t\t/* Currency Content */\n\t\t\tif (activityInfo.description.flags.NEEDS_CURRENCY_RESOLUTION) {\n\t\t\t\tif(that.contentBundle.currencies[activityInfo.counterparty.business]) {\n\t\t\t\t\tname = that.contentBundle.currencies[activityInfo.counterparty.business];\n\t\t\t\t}\n\t\t\t}\n\n\t\t\t//TODO: remove this if condition\n\t\t\t/**\n\t\t \t* This condition specifically satisfies DCC (Direct credit card) transactions requirement where we\n\t\t \t*\taren't suppose to show the paypal generated email id since there is no paypal account attached to the buyer CC.\n\t\t \t* sample counterparty field we get is  123jahsd@dcc.paypal.com. We should remove this code once service sends us a flag which helps us determine its\n\t\t \t*  DCC transaction or simply doesn't populate the counterpart field.\n\t\t \t**/\n\t\t\tif(name && name.substr(name.indexOf(\"@\")+1, name.length) === \"dcc.paypal.com\") {\n\t\t\t\tdescription = that.contentBundle.activity.PAYMENT;\n\t\t\t}else if (name !== '') {\n\t\t\t\tdescription = description + ' ' + name;\n\t\t\t}\n\n\t\t\tcallback(description);\n\t\t});\n\t},\n\n\t/**\n\t * @name: getTransactionTestActivities\n\t * @brief: Returns mock data to test the UI\n\t * @param: {object} payload\n\t * @param: {function} callback\n\t **/\n\tgetTransactionTestActivities: function(payload, callback) {\n\t\tvar result = {\n\t\t\tstatusCode: 200,\n\t\t\tbody: {\n\t\t\t\titems: require('../../test/mock/response/transactions/activityTypeResponsesWithExtensions')\n\t\t\t}\n\t\t};\n\n\t\tthis.activitySearchServCallback(false, null, result, null, callback);\n\t},\n\n\t/**\n\t * @name: constructServiceRequestTime\n\t * @brief: constructs ISO string request times for activitysearchserv\n\t * @param: year, month, day and boolean start/end of day\n\t **/\n\tconstructServiceRequestTime: function(year, month, day, start) {\n\t\tvar inputObj = {\n\t\t\tyear: +year,\n\t\t\tmonth: +month,\n\t\t\tday: +day,\n\t\t\thour: start ? 0 : 23,\n\t\t\tminute: start ? 0 : 59,\n\t\t\tsecond: start ? 0 : 59\n\t\t};\n\n\t\tvar time = Moment.tz(inputObj, this.locality.timezone).toISOString();\n\n\t\treturn time;\n\t},\n\n\t/**\n\t * @name: getTransactionActivities\n\t * @brief: Calls activitysearchserv, builds the required payload.\n\t * @param: {object} payload\n\t * @param: {function} callback\n\t **/\n\tgetTransactionActivities: function(payload, callback) {\n\t\tvar account = payload.account,\n\t\t\tsecondaryHash={},\n\t\t\tsecondaryAuxInfo={\n\t\t\t\tsecondaryHash:null,\n\t\t\t\tauxInfo:null,\n\t\t\t\tisPrimaryTxn:true\n\t\t\t},\n\t\t\tpopulateBalance = (payload.query.transactiontype === 'BALANCE_AFFECTING_TRANSACTIONS'),\n\t\t\tSERVICE_NAME = \"activitysearchserv\",\n\t\t\tserviceInfo = {\n\t\t\t\tserviceName: SERVICE_NAME,\n\t\t\t\tendPoint: '',\n\t\t\t\tparams: {\n\t\t\t\t\tis_internal_client: true,\n\t\t\t\t\tneed_hold_info: true\n\t\t\t\t}\n\t\t\t},\n\t\t\tsecondaryServiceInfo ={\n\t\t\t\tserviceName: SERVICE_NAME,\n\t\t\t\tendPoint: \"payment-activities\",\n\t\t\t\tparams: {\n\t\t\t\t\tis_internal_client: true,\n\t\t\t\t\tneed_actions: true,\n\t\t\t\t\tneed_shipping_info: true,\n\t\t\t\t\tneed_hold_info: true,\n\t\t\t\t\tneed_primary_only:false\n\t\t\t\t}\n\t\t\t},\n\t\t\tthat=this,\n\t\t\tfilterEndpoint;\n\n\t\t/* Set the default start time to a month ago for activities and 3 months for search, unless it was passed in */\n\t\tif (payload.query.fromdate_year && payload.query.fromdate_month && payload.query.fromdate_day) {\n\t\t\tpayload.query.start_time = this.constructServiceRequestTime(\n\t\t\t\tpayload.query.fromdate_year,\n\t\t\t\tpayload.query.fromdate_month,\n\t\t\t\tpayload.query.fromdate_day, true);\n\t\t} else {\n\t\t\tif ((typeof(payload.query.defaultOption) !== \"undefined\") && payload.query.defaultOption === \"Business_Hawk_Transactions_Search_90_day\") {\n\t\t\t\tpayload.query.start_time = Moment().tz(this.locality.timezone).subtract(3, 'months').toISOString();\n\t\t\t} else {\n\t\t\t\tpayload.query.start_time = Moment().tz(this.locality.timezone).subtract(1, 'months').toISOString();\n\t\t\t}\n\t\t}\n\n\t\t/* Set the default end time to note: end of day\n\t\t */\n\t\tif (payload.query.todate_year && payload.query.todate_month && payload.query.todate_day) {\n\t\t\tpayload.query.end_time = this.constructServiceRequestTime(\n\t\t\t\tpayload.query.todate_year,\n\t\t\t\tpayload.query.todate_month,\n\t\t\t\tpayload.query.todate_day, false);\n\t\t} else {\n\t\t\tpayload.query.end_time = Moment().tz(this.locality.timezone).endOf('day').toISOString();\n\t\t}\n\n\t\t/* Figure out the right endpoints and the params they need */\n\t\tfilterEndpoint = this.buildFilters(payload.query, serviceInfo.params);\n\t\tserviceInfo.endPoint = (typeof filterEndpoint === \"string\" && filterEndpoint !== \"\") ? filterEndpoint : \"activities\";\n\n\t\t/* Get all the params, and clean up the empty ones */\n\t\t_.extend(serviceInfo.params, payload.query,\n\t\t\t/* Set page_size only if it isn't passed through */\n\t\t\tpayload.query.page_size ? {} : {\n\t\t\t\tpage_size: Constants.transactions.FETCH_COUNT\n\t\t\t}\n\t\t);\n\n\t\tserviceInfo.params = Utils.cleanRequestParams(serviceInfo.params);\n\t\t// Call ActivitySearchServ\n\t\tcal.logInfo('ACTIVITY_SEARCH_SERV_PARAMS accountNumber: ' + account.accountNumber, serviceInfo.params);\n\n\t\tasync.waterfall([\n\t\t\tfunction getPrimaryTransactionList(callback){\n\t\t\t\t//TODO: check for statusCode here too.\n\t\t\t\tthis.serviceHelper.initiateCall(serviceInfo, function(err, result) {\n\t\t\t\t\tif (err) {\n\t\t\t\t\t\tcal.logAsSystemError(TxnConstants.CalErrorMessage + \"Transactions Page: activityMiddleware error\");\n\t\t\t\t\t\tcal.logAsSystemError(\"getTransactionActivities::getPrimaryTransactionList:::ERR\", err);\n\t\t\t\t\t}\n\t\t\t\t\tcallback(err,result);\n\t\t\t\t}.bind(this));\n\t\t\t}.bind(this),\n\t\t\tfunction getSecondaryTransactionList(resultObject,callback){\n\t\t\t\tif (resultObject && resultObject.body.items && resultObject.body.items.length) {\n\t\t\t\t_.extend(secondaryServiceInfo.params, {\n\t\t\t\t\t\tpayment_ids: resultObject