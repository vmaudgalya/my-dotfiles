 any activities that shouldn't be available\n\t\t\t\tif (!Constants.transactionExludedActions[actionItem]) {\n\t\t\t\t\tvar action = new ActivityAction(),\n\t\t\t\t\t\tactionIdentifier = actionItem;\n\n\t\t\t\t\taction.name = this.contentBundle.actions[actionIdentifier];\n\t\t\t\t\taction.type = actionIdentifier;\n\t\t\t\t\taction.url = Constants.transactionActionUrl[actionIdentifier] +\n\t\t\t\t\t\t((activity.activity_type === \"INVOICE\") ? activity.id : activity.internal_id);\n\n\t\t\t\t\t/* If the item was sold via eBay, set that param in the URL.\n\t\t\t\t\t * This is so that the merchant is directed to an ebay shipping\n\t\t\t\t\t * flow in such a case.\n\t\t\t\t\t */\n\t\t\t\t\tif (actionItem === \"SHIPPING_SHIP\" && isEbayTxn) {\n\t\t\t\t\t\taction.url = action.url.replace(Constants.notEbayTxnUrlParam, Constants.ebayTxnUrlParam);\n\t\t\t\t\t}\n\t\t\t\t\taction.isNewWindow = (actionItem === 'PAYMENT_TRACK_SHIPPING');\n\t\t\t\t\tif (typeof action.name !== \"undefined\") {\n\t\t\t\t\t\tactionList.push(action);\n\t\t\t\t\t} else {\n\t\t\t\t\t\t/* Error: unknown action */\n\t\t\t\t\t\tcal.logAsSystemError(TxnConstants.CAL_ERR_MESSAGE + \": Unknown action:\", action.type);\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}.bind(this));\n\t\t\tif(isPrimaryTxn){\n\t\t\t\tactionList.push(this.addArchiveActions(activity)); //To enable archive/unarchive\n\t\t\t}\n\t\t}\n\t\treturn actionList;\n\n\t},\n\n\t/**\n\t * @name: getActionListSortFunction\n\t * @brief: return sort function for action array\n\t * @return: {function|null}\n\t **/\n\tgetActionListSortFunction: function(subtype, type) {\n\t\tif (subtype === 'PAYMENT' && type === 'PAYMENT') {\n\t\t\treturn function shipmentSortFunction(firstActivityAction, secondActivityAction) {\n\t\t\t\tvar sortOrderActivityActionType = [\n\t\t\t\t\t'SHIPPING_SHIP',\n\t\t\t\t\t'ADD_TRACKING',\n\t\t\t\t\t'MARK_AS_SHIPPED',\n\t\t\t\t\t'REFUND',\n\t\t\t\t\t'HIDE',\n\t\t\t\t\t'SHOW'\n\t\t\t\t];\n\n\t\t\t\t//type may be one of the strings from sortOrde